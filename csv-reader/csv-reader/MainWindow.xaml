<Window x:Class="csv_reader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:csv_reader"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Button_Data_Ordered" Content="Show ordered data" Click="Click_ShowDateOrderedData" HorizontalAlignment="Center" Height="37" VerticalAlignment="Top" Width="800"/>
        <Button x:Name="Button_Data_Summary" Content="Show item summary" Click="Click_ShowItemSummary" HorizontalAlignment="Center" Height="37" VerticalAlignment="Top" Width="800" Margin="0,42,0,0"/>
        <TextBox x:Name="Output_Box" Text="" HorizontalAlignment="Center" Height="340" Margin="0,84,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="780"/>

    </Grid>
</Window>
